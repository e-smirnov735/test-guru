<h2><%= t('.test') %>: <%= @question.test.title %></h2>
<p><%= t('.question') %>: <%= @question.body %></p>
<p><%= t('.number') %>: <%= @question.test_id %>"</p>
<h2><%= t('.answers') %></h2>
<%= link_to t('.new_answer'), new_admin_question_answer_path(@question),
    class: 'btn btn-primary' %>
<div>
  <%= render @question.answers %>
</div>
<p>
  <%= link_to t('back'), admin_test_path(@question.test_id), class: 'btn btn-success' %>
</p>
